<?xml version="1.0" encoding="utf-8"?>
                                                                   

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#fcfcfc"
        android:orientation="vertical" android:tag="layout/fragment_completed_referral_received_0" xmlns:android="http://schemas.android.com/apk/res/android">

        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/rvRequirement"
            android:layout_marginTop="@dimen/dp5"
            android:visibility="gone"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:overScrollMode="never" />

        <LinearLayout
            android:visibility="gone"
            android:id="@+id/NoLeadInvited"
            android:layout_width="match_parent"
            android:layout_gravity="center"
            android:gravity="center"
            android:layout_height="match_parent"
            android:orientation="vertical">


            <TextView
                android:id="@+id/default_text"
                android:layout_marginTop="@dimen/dp15"
                android:layout_marginLeft="@dimen/dp10"
                android:layout_marginRight="@dimen/dp10"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/notosans_jp_bold"
                android:gravity="left"
                android:text="No Guest Invited"
                android:textColor="@color/black"
                android:textSize="22dp" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/linearlayout1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"

            android:orientation="vertical" android:tag="binding_1">
            <include layout="@layout/ab_requirements" />
            <include layout="@layout/ab_requirements" />
            <include layout="@layout/ab_requirements" />




        </LinearLayout>
    </LinearLayout>
         